void	free_all(char ****gstr, int **graph)
{
	(*graph[0]) = -1;
	while (++graph[0] < (graph[3] - 1))
	{
		(*graph[1]) = 0;
		while ((***gstr[graph[0]][graph[1]]) != 0)
		{
			free(gstr[graph[0]][graph[1]]);
			(*graph[1]) += 1;
		}
		free(gstr[graph[0]]);
	}
	free(gstr);
}

